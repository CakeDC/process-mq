{
    "name": "friendsofcake/process-mq",
    "description": "A plugin for handling connections and jobs using RabbitMQ",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "queue",
        "rabbit-mq"
    ],
    "homepage": "https://github.com/friendsofcake/process-mq",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-zlib": "*",
        "ext-json": "*",
        "cakephp/cakephp": "5.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ProcessMQ\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProcessMQ\\Test\\": "tests"
        }
    }
}
