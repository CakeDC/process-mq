{
    "name": "friendsofcake/process-mq",
    "description": "A plugin for handling connections and jobs using RabbitMQ",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "queue",
        "rabbit-mq"
    ],
    "homepage": "https://github.com/friendsofcake/process-mq",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-zlib": "*",
        "ext-json": "*",
        "cakephp/cakephp": "^5.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1.0",
        "cakephp/cakephp-codesniffer": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "ProcessMQ\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProcessMQ\\Test\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
